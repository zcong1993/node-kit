  },
  "types": "./dist/index.d.ts",
  "author": "zcong1993",
  "license": "MIT",
  "devDependencies": {
    "@microsoft/api-extractor": "^7.18.4",
    "@nestjs/common": "^8.0.5",
    "@types/ioredis": "^4.26.6",
    "@types/jest": "^27.0.0",
    "@types/node": "^16.4.6",
    "@types/promise.allsettled": "^1.0.3",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "@zcong/node-redis-cache": "^0.7.2",
    "chalk": "^4.1.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "execa": "^5.1.1",
    "husky": "^7.0.1",
    "ioredis": "^4.27.7",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.1",
    "minimist": "^1.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "prom-client": "^13.1.0",
    "prompts": "^2.4.1",
    "reflect-metadata": "^0.1.13",
    "semver": "^7.3.5",
    "supertest": "^6.1.4",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  },
  "lint-staged": {
    "*": "prettier --write"
  },
  "publishConfig": {
