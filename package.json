{
  "name": "@zcong/node-kit",
  "version": "0.18.0",
  "description": "Programming toolkit for NodeJS.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s ts-build api-types",
    "test": "run-s lint jest",
    "test:cov": "run-s lint jest:cov",
    "ts-build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "jest": "vitest",
    "jest:cov": "vitest run --coverage",
    "jest:local": "npm run jest -- -o",
    "prepublishOnly": "npm run build",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "api-types": "api-extractor run",
    "release": "bumpp --commit --push --tag"
  },
  "author": "zcong1993",
  "license": "MIT",
  "devDependencies": {
    "@microsoft/api-extractor": "^7.24.1",
    "@nestjs/common": "^9.1.5",
    "@types/hashring": "^3.2.1",
    "@types/imurmurhash": "^0.1.1",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.11.4",
    "@types/promise.allsettled": "^1.0.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@vitest/coverage-c8": "^0.29.0",
    "@zcong/js-release": "^0.2.5",
    "@zcong/node-redis-cache": "^0.9.1",
    "buffer-crc32": "^0.2.13",
    "bumpp": "^9.0.0",
    "c8": "^7.12.0",
    "chalk": "^5.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "conventional-changelog-cli": "^2.2.2",
    "date-fns": "^2.29.3",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "execa": "^7.0.0",
    "hashring": "^3.2.0",
    "husky": "^8.0.1",
    "imurmurhash": "^0.1.4",
    "ioredis": "^5.2.3",
    "jsonwebtoken": "^9.0.0",
    "lint-staged": "^13.0.3",
    "minimist": "^1.2.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "prom-client": "^14.1.0",
    "prompts": "^2.4.2",
    "reflect-metadata": "^0.1.13",
    "semver": "^7.3.8",
    "supertest": "^6.3.0",
    "ts-node": "^10.9.1",
    "tsup": "^6.3.0",
    "typescript": "^5.0.0",
    "vitest": "^0.29.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@zcong/singleflight": "^0.3.1",
    "cockatiel": "^3.0.0",
    "iterare": "^1.2.1",
    "lodash": "^4.17.21",
    "rxjs": "^7.5.7"
  },
  "peerDependencies": {
