{
  "name": "@zcong1993/node-kit",
  "version": "0.7.0",
  "description": "my cool project",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s tsc-build api-types",
    "test": "run-s lint jest",
    "test:cov": "run-s lint jest:cov",
